	"license": "UNLICENSED",
	"private": true,
	"dependencies": {
		"@total-typescript/ts-reset": "^0.5.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.23.8",
		"@babel/preset-typescript": "^7.23.3",
		"@commitlint/cli": "^18.4.4",
		"@parcel/transformer-sass": "^2.11.0",
		"@testing-library/jest-dom": "^6.2.0",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.2",
		"@types/node": "^20.11.5",
		"@types/react": "^18.2.48",
		"@types/react-dom": "^18.2.18",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.19.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"identity-obj-proxy": "^3.0.0",
		"is-ci": "^3.0.1",
		"jest": "^29.7.0",
		"jest-environment-jsdom": "^29.7.0",
		"jest-extended": "^4.0.2",
		"lint-staged": "^15.2.0",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.11.0",
		"pinst": "^3.0.0",
		"prettier": "^3.2.4",
		"process": "^0.11.10",
		"stylelint": "^16.1.0",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-standard-scss": "^13.0.0",
		"stylelint-order": "^6.0.4",
		"stylelint-prettier": "^5.0.0",
		"typescript": "^5.3.3"
	},
	"scripts": {
		"build": "npm run typecheck && node ./scripts/prebuild.js && parcel build --no-autoinstall --public-url \".\" --no-source-maps && node ./scripts/postbuild.js",
		"develop": "node ./scripts/prebuild.js && parcel serve --target default --no-autoinstall --lazy --dist-dir \"./build\" --https",
		"lint": "npm-run-all \"lint:*\" --parallel --continue-on-error --aggregate-output",
		"lint:scss": "stylelint \"./src/**/*.scss\"",
		"lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
		"postinstall": "is-ci || husky install",
		"postpublish": "pinst --enable",
		"prepublishOnly": "pinst --disable",
		"test": "jest --coverage",
		"test-watch": "jest --watch",
		"typecheck": "tsc"
	},
	"source": "./src/index.html",
	"targets": {
		"default": {
			"distDir": "./build"
		},
		"server": {
			"source": "./src/server.ts",
			"isLibrary": true,
			"context": "node",
			"outputFormat": "commonjs",
			"distDir": "./build/server"
		}
	}
}
