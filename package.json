{
	"name": "__PLACEHOLDER__",
	"version": "0.0.0",
	"author": "__PLACEHOLDER__",
	"license": "UNLICENSED",
	"private": true,
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.21.0",
		"@commitlint/cli": "^17.4.4",
		"@parcel/transformer-sass": "^2.8.3",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/node": "^18.15.3",
		"@types/react": "^18.0.28",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"babel-plugin-module-resolver": "^5.0.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"identity-obj-proxy": "^3.0.0",
		"is-ci": "^3.0.1",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"jest-extended": "^3.2.4",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.8.3",
		"pinst": "^3.0.0",
		"prettier": "^2.8.4",
		"pretty-quick": "^3.1.3",
		"process": "^0.11.10",
		"stylelint": "^15.2.0",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-standard-scss": "^7.0.1",
		"stylelint-order": "^6.0.3",
		"stylelint-prettier": "^3.0.0",
		"typescript": "^4.9.5"
	},
	"scripts": {
		"build": "npm run typecheck && node ./scripts/prebuild.js && parcel build --public-url \".\" --no-source-maps && node ./scripts/postbuild.js",
		"develop": "node ./scripts/prebuild.js && parcel serve --target default --no-autoinstall --lazy --dist-dir \"./build\" --https",
		"lint": "npm-run-all \"lint:*\" --parallel --continue-on-error --aggregate-output",
		"lint:scss": "stylelint \"./src/**/*.scss\"",
		"lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
		"postinstall": "is-ci || husky install",
		"postpublish": "pinst --enable",
		"prepublishOnly": "pinst --disable",
		"test": "jest --coverage",
		"test-watch": "jest --watch",
		"typecheck": "tsc --noEmit"
	},
	"source": "./src/index.html",
	"targets": {
		"default": {
			"distDir": "./build"
		},
		"server": {
			"source": "./src/server.ts",
			"isLibrary": true,
			"context": "node",
			"outputFormat": "commonjs",
			"distDir": "./build/server"
		}
	}
}
