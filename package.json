{
	"name": "__PLACEHOLDER__",
	"version": "0.0.0",
	"author": "__PLACEHOLDER__",
	"license": "UNLICENSED",
	"private": true,
	"dependencies": {
		"@total-typescript/ts-reset": "^0.4.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.22.9",
		"@babel/preset-typescript": "^7.22.5",
		"@commitlint/cli": "^17.6.7",
		"@parcel/transformer-sass": "^2.9.3",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^14.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/node": "^20.4.8",
		"@types/react": "^18.2.18",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"babel-plugin-module-resolver": "^5.0.0",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.33.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"husky": "^8.0.3",
		"identity-obj-proxy": "^3.0.0",
		"is-ci": "^3.0.1",
		"jest": "^29.6.2",
		"jest-environment-jsdom": "^29.6.2",
		"jest-extended": "^4.0.1",
		"lint-staged": "^13.2.3",
		"npm-run-all": "^4.1.5",
		"parcel": "^2.9.3",
		"pinst": "^3.0.0",
		"prettier": "^3.0.1",
		"process": "^0.11.10",
		"stylelint": "^15.10.2",
		"stylelint-config-rational-order": "^0.1.2",
		"stylelint-config-standard-scss": "^10.0.0",
		"stylelint-order": "^6.0.3",
		"stylelint-prettier": "^4.0.2",
		"typescript": "^5.1.6"
	},
	"scripts": {
		"build": "npm run typecheck && node ./scripts/prebuild.js && parcel build --no-autoinstall --public-url \".\" --no-source-maps && node ./scripts/postbuild.js",
		"develop": "node ./scripts/prebuild.js && parcel serve --target default --no-autoinstall --lazy --dist-dir \"./build\" --https",
		"lint": "npm-run-all \"lint:*\" --parallel --continue-on-error --aggregate-output",
		"lint:scss": "stylelint \"./src/**/*.scss\"",
		"lint:ts": "eslint \"./src/**/*.{ts,tsx}\"",
		"postinstall": "is-ci || husky install",
		"postpublish": "pinst --enable",
		"prepublishOnly": "pinst --disable",
		"test": "jest --coverage",
		"test-watch": "jest --watch",
		"typecheck": "tsc --noEmit"
	},
	"source": "./src/index.html",
	"targets": {
		"default": {
			"distDir": "./build"
		},
		"server": {
			"source": "./src/server.ts",
			"isLibrary": true,
			"context": "node",
			"outputFormat": "commonjs",
			"distDir": "./build/server"
		}
	}
}
